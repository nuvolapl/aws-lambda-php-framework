{
    "name": "nuvolapl/aws-lambda-php-framework",
    "type": "library",
    "description": "AWS Lambda PHP Framework",
    "authors": [
        {
            "name": "Bartek Chmura",
            "email": "bartek@nuvola.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nuvola\\AwsLambdaFramework\\": "src/",
            "Test\\Nuvola\\AwsLambdaFramework\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
